{
    "name": "dynamic/silverstripe-base-site-migrator",
    "description": "A skeleton for SilverStripe modules.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "dynamic/silverstripe-base-site": "^4.0",
        "dynamic/silverstripe-company-config": "^1.0@dev",
        "dynamic/silverstripe-integration-config": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\BaseMigrator\\": "src/",
            "Dynamic\\BaseMigrator\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
